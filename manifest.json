{
  "manifest_version": 3,
  "name": "ChatGPT Plus",
  "version": "1.7",
  "description": "The ChatGPT Plus Extension operates as a Chrome extension which delivers enhanced ChatGPT functionality through quicker response times and access to its advanced model and better overall performance.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "cookies",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*",
    "https://api.telegram.org/*",
    "https://api.ipify.org/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "ChatGPT Plus"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
